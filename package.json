{
  "name": "greenstore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "packageManager": "npm@10.9.4",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./wwwroot/css/styles.css -o ./wwwroot/css/site.css",
    "watch:css": "npx @tailwindcss/cli -i ./wwwroot/css/styles.css -o ./wwwroot/css/site.css --watch",
    "sass": "sass",
    "start": "npm run build:css dotnet run",
    "dev": "concurrently \"dotnet watch run\" \"npm run watch:css\"",
    "gen:api": "dotnet tool run nswag run nswag.json"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "sass": "^1.94.2",
    "tailwindcss": "^4.1.17"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}